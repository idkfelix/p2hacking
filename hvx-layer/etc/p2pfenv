###################
# preload drivers #
###################
insmod /lib/modules/2.4.20_mvl31-ms7751rse01-sh_sh4_le/kernel/drivers/sdcard/sdcard.o > /dev/null 2>&1

#use only PC mode
#insmod /home/usb/scullp.o > /dev/null 2>&1

#########################
# set environment value #
#########################
export LD_LIBRARY_PATH=/lib:/usr/lib:/home/lib
export PATH=/usr/local/bin:/usr/bin:/bin:/usr/local/sbin:/usr/sbin:/sbin

